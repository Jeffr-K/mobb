{
  "printWidth": 120,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "auto",
  "semi": true,
  "bracketSameLine": true,
  "singleAttributePerLine": false,
  "functionParentheses": false,
  "arrowSpacing": ["error", { "before": true, "after": true }],
  "useTabs": false,
  "tabWidth": 2
}